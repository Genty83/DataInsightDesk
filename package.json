    "react-router-dom": "^7.6.3",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/electron": "^1.6.12",
    "@types/node": "^24.0.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
